{
  "name": "rectron",
  "version": "1.0.8",
  "author": "Maks1mio <one.evt@gmail.com>",
  "description": "A modern nHentai client built with Electron",
  "main": ".webpack/main",
  "license": "GPL-3.0+",
  "type": "commonjs",
  "scripts": {
    "postinstall": "rimraf node_modules/bufferutil node_modules/utf-8-validate",
    "start": "cross-env WS_NO_BUFFER_UTIL=1 WS_NO_UTF8_VALIDATE=1 electron-forge start",
    "build": "electron-builder --win --publish always",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "build_prod": "electron-forge package && electron-builder --config electron-builder.yml --publish never",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/Maks1mio/NHApp",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.nhapp.rectron",
    "productName": "Rectron",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@emotion/is-prop-valid": "^1.3.1",
    "@emotion/react": "^11.14.0",
    "@types/react-router-dom": "^5.3.3",
    "assert": "^2.1.0",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.0.0",
    "cls": "^0.1.5",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.5.0",
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "framer-motion": "^12.12.2",
    "https-browserify": "^1.0.0",
    "jq": "^1.7.2",
    "nhentai-api": "^3.4.3",
    "node-cache": "^5.1.2",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "os-browserify": "^0.3.0",
    "react": "^19.0.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.16.3",
    "react-pageflip": "^2.0.3",
    "react-responsive-masonry": "^2.7.1",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.6.0",
    "react-window": "^1.8.11",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.6.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@vercel/webpack-asset-relocator-loader": "1.5.0",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^34.0.0",
    "electron-builder": "^26.0.12",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "config": {
    "forge": "./forge.config.ts"
  }
}
